# Configurações do Banco de Dados
POSTGRES_DB=rotas_db
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/rotas_db

# Configurações da API
API_V1_STR=/api/v1
PROJECT_NAME=Sistema de Cálculo de Rotas
ENVIRONMENT=development

# Configurações de CORS
BACKEND_CORS_ORIGINS=["http://localhost:5000","http://localhost:5001","http://127.0.0.1:5000","http://127.0.0.1:5001"]

# Configurações de Cache
CACHE_EXPIRATION_HOURS=6
COORDINATES_CACHE_EXPIRATION_HOURS=168

# APIs Externas
ROTAS_BRASIL_API_URL=https://rotasbrasil.com.br/roterizador/buscaRota/

# PgAdmin (opcional)
PGADMIN_EMAIL=admin@routecalc.com
PGADMIN_PASSWORD=admin
